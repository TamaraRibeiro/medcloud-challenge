generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Patient {
  id String @id @default(uuid())

  name          String
  email         String
  birth         String
  cpf           String?
  address       String
  cep           String
  addressNumber String
  complement    String?
  neighbourhood String
  city          String?
  uf            String?
  country       String?
}
